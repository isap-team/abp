        {
          "Host": "localhost",
          "Port": 44301
        }
      ],
      "UpstreamPathTemplate": "/.well-known/openid-configuration",
      "UpstreamHttpMethod": [ "Put", "Delete", "Get", "Post" ]
    },
    {
