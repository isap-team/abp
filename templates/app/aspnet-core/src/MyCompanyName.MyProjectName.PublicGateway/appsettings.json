{
  "App": {
    "SelfUrl": "https://localhost:44310",
    "CorsOrigins": "https://*.vicon.com,https://localhost:7000,http://localhost:4200"
  },
  "AuthServer": {
    "Authority": "https://localhost:44301",
    "ApiName": "Vicon"
  },
  "RemoteServices": {
    "Default": {
      "BaseUrl": "https://localhost:44300/"
    }
  },
  "Redis": {
